{
  "name": "flux-minimal",
  "displayName": "Flux Minimal",
  "description": "Minimal Flux language support with automated changelog - testing automation",
  "version": "0.1.4",
  "publisher": "flux-dev",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "flux",
        "aliases": [
          "Flux",
          "flux-lang"
        ],
        "extensions": [
          ".fx"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "flux",
        "scopeName": "source.flux",
        "path": "./syntaxes/flux-minimal.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "id": "flux-dark",
        "label": "Flux Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/flux-dark-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "flux-icons",
        "label": "Flux File Icons",
        "path": "./icons/flux-icon-theme.json"
      }
    ],
    "icons": {
      "flux-file": {
        "description": "Flux file icon",
        "default": {
          "iconPath": "./icon.png"
        }
      }
    }
  }
}
