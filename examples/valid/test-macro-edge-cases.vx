;; Test edge cases for macro definition removal

;; Case 1: Multiple macro definitions
(macro first [x] (def result1 ~x))
(macro second [y] (def result2 ~y))

;; Case 2: Macro definition with complex body
(macro complex [name body] (def ~name (fn [] (do ~body 42))))

;; Case 3: Using the macros
(first "hello")
(second "world")
(complex test (+ 1 2))